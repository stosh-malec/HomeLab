#All Aliases 
alias set_alias='nano $HOME/.bashrc && sudo cp $HOME/.bashrc /root/.bashrc > /dev/null 2>&1 && sudo cp $HOME/.bashrc /home/stoshmalec/.bashrc > /dev/null 2>&1 && source $HOME/.bashrc'
alias s='source ~/.bashrc'
alias c='clear'
alias k='kubectl'
alias gc='git commit -m'
alias gs='git status'
alias gpu='git pull'
alias gp='git push'
alias g='git'
alias ga='git add *'
alias gh='github'
alias gd='git diff'
alias gst='git stash'